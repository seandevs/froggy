<h1>Update Your Info</h1>
<%= error_messages_for 'user' %> 
<% form_for @user, :html => { :multipart => true } do |f| %>
<p>
  name:<br />
  <%= f.text_field :name %>
</p>
<p>
  email:<br />
  <%= f.text_field :email %>
</p>
<p>
  username:<br />
  <%= f.text_field :login %>
</p>
<p>
  password:<br />
  <%= f.text_field :password %>
</p>
<p>
  <%= f.file_field :photo %>
</p>	
<p>
  <%= f.submit "Update" %>
</p>
<% end %>